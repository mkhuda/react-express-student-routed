{
  "name": "student-app-react-express",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npm install -g knex",
    "heroku-postbuild": "knex migrate:latest && knex seed:run && cd app && npm install",
    "app": "cd app && npm start",
    "server": "node server.js",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run app\""
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect": "^3.6.6",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "knex": "^0.14.5",
    "method-override": "^2.3.10",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  }
}
