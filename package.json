{
  "name": "student-app-react-express",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm install && npm install --prefix app && npm install knex -g && knex migrate:latest && knex seed:run",
    "app": "cd app && npm start",
    "server": "nodemon server.js",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run app\""
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect": "^3.6.6",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "knex": "^0.14.5",
    "method-override": "^2.3.10",
    "mysql2": "^1.5.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
